@article{woodLandsurfaceHydrologyParameterization1992,
  title = {A Land-Surface Hydrology Parameterization with Subgrid Variability for General Circulation Models},
  author = {Wood, Eric F. and Lettenmaier, Dennis P. and Zartarian, Valerie G.},
