@article{bhadraDevelopmentGeomorphologicalInstantaneous2008,
  title = {Development of a Geomorphological Instantaneous Unit Hydrograph Model for Scantily Gauged Watersheds},
  author = {Bhadra, A. and Panigrahy, N. and Singh, R. and Raghuwanshi, N.S. and Mal, B.C. and Tripathi, M.P.},
  date = {2008-08},
  journaltitle = {Environmental Modelling \& Software},
  shortjournal = {Environmental Modelling \& Software},
  volume = {23},
  number = {8},
  pages = {1013--1025},
  issn = {13648152},
  doi = {10.1016/j.envsoft.2007.08.008},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S1364815207002034},
  urldate = {2022-09-14},
  abstract = {In recent years, geomorphological instantaneous unit hydrograph (GIUH) approach to estimate the runoff from a watershed due to different storm patterns has gained wide acceptability for watersheds with scanty data. In this study, a user-friendly event based computerized GIUH model, GIUH\_CAL, was developed. GIUH\_CAL model incorporates an infiltration component. Depending on the availability of soil and infiltration data, the model offers a choice among three methods of infiltration calculation, viz., Richards’ equation, phi-index method and Philip two term model. The graphical user interface (GUI) of the model is based on mouse-driven approach with pop-up windows, pull-down menus, sub-menus, toolbar, statusbar and button controls including comprehensive context-sensitive help file. GIUH\_CAL model displays generated instantaneous unit hydrograph (IUH), unit hydrograph (UH) and direct runoff hydrograph (DRH) of the watershed graphically and processes statistically. Modelling efficiency (ME), coefficient of residual mass (CRM), root mean square error (RMSE) and coefficient of determination (r2) are used as the performance criteria. The model was tested for two distinctly different watersheds, i.e., a 4th order Guptamani watershed in tropical sub-humid region and a 3rd order Upper Chhokeranala watershed in semi-arid region of India. The results show that model performs satisfactorily for both test watersheds and performance is much better with Richards’ equation option of infiltration calculation as compared to phi-index method and Philip two term model.Thus, GIUH\_CAL model can be adopted as a standard tool for modelling rainfallerunoff transformation process in scantily gauged watersheds.},
  langid = {english},
  file = {C\:\\Users\\Kan_Lei\\Zotero\\storage\\735EKE93\\Bhadra et al. - 2008 - Development of a geomorphological instantaneous un.pdf}
}

